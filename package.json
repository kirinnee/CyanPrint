	  "@types/inquirer":     "0.0.43",
	  "@types/mocha":        "^5.2.5",
	  "@types/node":         "^10.12.2",
	  "@types/relaxed-json": "^1.0.0",
	  "@types/rimraf":       "^2.0.2",
	  "@types/webpack":      "^4.4.17",
	  "chai":                "^4.2.0",
	  "mocha":               "^5.2.0",
	  "nyc":                 "^13.0.1",
    "terser-webpack-plugin": "^1.1.0",
	  "ts-loader":           "^5.2.2",
	  "ts-node":             "^7.0.1",
	  "typescript":          "^3.1.3",
	  "webpack":             "^4.20.2",
	  "webpack-cli":         "^3.1.2"
  }
