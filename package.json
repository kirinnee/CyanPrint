	  "cli-progress":      "^2.1.0",
	  "commander":         "^2.19.0",
	  "fs-extra":          "^7.0.1",
	  "glob":              "^7.1.3",
	  "graceful-fs":       "^4.1.11",
	  "inquirer":          "^6.2.0",
	  "node-fetch":        "^2.3.0",
	  "relaxed-json":      "^1.0.1",
	  "rimraf":            "^2.6.2"
  },
  "devDependencies": {
	  "@types/chai":         "^4.1.6",
	  "@types/cli-progress": "^1.8.0",
	  "@types/fs-extra":     "^5.0.5",
	  "@types/glob":         "^7.1.1",
