{
  "name": "cyanprint",
  "version": "0.12.9",
  "description": "CyanPrint is a scaffolding CLI for people to create templates or blueprints to scaffold commonly used ",
  "main": "dist/cyan.js",
  "license": "MIT",
  "author": {
    "name": "kirinnee",
    "email": "kirinnee@gmail.com"
  },
  "types": "./types/cyan.d.ts",
  "bin": {
    "cyan": "dist/cyan.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kirinnee/CyanPrint.git"
  },
  "scripts": {
    "deploy": "node script deploy",
    "build": "node script wp dist",
    "dev": "node script wp dist -w",
    "test": "node script test",
    "test:w": "node script test -w",
    "coverage": "node script test --cover",
    "patch": "node script publish patch",
    "minor": "node script publish minor",
    "major": "node script publish major",
    "start": "node dist/cyan"
  },
  "dependencies": {
	  "@kirinnee/core":    "^0.7.0",
