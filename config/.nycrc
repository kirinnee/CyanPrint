{
  "check-coverage": true,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/cyan.ts",
    "**/*.spec.ts",
    "**/*.d.ts"
  ],
  "extension": [
    ".ts"
  ],
  "register": [
    "ts-node/register"
  ],
  "all": true,
  "lines": 80,
  "statements": 0,
  "functions": 100,
  "branches": 100,
  "cache":false,
  "instrument": true,
  "sourceMap": true
}